---
layout: nil
---
<?xml version="1.0"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
    <channel>
        <title>potomushto.com</title>
        <link>http://potomushto.com/</link>
        <atom:link href="http://potomushto.com/rss.xml" rel="self" type="application/rss+xml" />
        <description>Live in Russia, work in IT</description>
        <language>ru-ru</language>
        <pubDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
        <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>

        {% for post in site.posts %}
        <item>
            <title>{{ post.title }}</title>
            <link>http://potomushto.com{{ post.url }}</link>
            <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
            <author>unknownliveid@hotmail.com (potomushto)</author>
            <guid>http://potomushto.com{{ post.id }}</guid>
            <description>{{ post.content | xml_escape }}</description>
        </item>
        {% endfor %}

    </channel>
</rss>
